/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
	Not(in = sel , out = sel1);
    And(a = a,b = sel1 , out = op1);
	And(a = b , b = sel , out = op2);
	Or(a = op1 , b = op2 , out = out);
}
